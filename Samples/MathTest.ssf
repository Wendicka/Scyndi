program MathTest
Use 
	"Math"
end

begin
	for i=1,100
		writeln ""
		writeln "Number:      ",i
		writeln "Sine:        ",sin(i)
		writeln "Cosine:      ",cos(i)
		writeln "Square Root: ",sqr(i)
		writeln "Random num:  ",rand(0,i)
		writeln "Tangent:     ",tan(i)
		writeln "Quadrat:     ",pow(i,2)
	end
	for i=0,5
		writeln "Factorial ",i," => ",Factorial(i)
	end
	writeln "Pi = ",PI
end